{
  "name": "is-it-that-much",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "db:push": "ls -1a | grep \"^.env*\" | fzf | xargs -I {} dotenv -e {} -- prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "ls -1a | grep \"^.env*\" | fzf | xargs -I {} dotenv -e {} -- prisma db seed",
    "db:fill": "dotenv -e .env -- tsx prisma/fill.ts",
    "db:reset": "ls -1a | grep \"^.env*\" | fzf | xargs -I {} dotenv -e {} -- tsx prisma/reset.ts",
    "db:export": "ls -1a | grep \"^.env*\" | fzf | xargs -I {} dotenv -e {} -- ./scripts/export-to-csv",
    "db:import": "ls -1a | grep \"^.env*\" | fzf | xargs -I {} dotenv -e {} -- ./scripts/import-from-csv",
    "db:migrate:dev": "dotenv -e .env -- prisma migrate dev",
    "db:migrate:deploy": "ls -1a | grep \"^.env*\" | fzf | xargs -I {} dotenv -e {} -- prisma migrate deploy",
    "dev": "next dev",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "prisma:dev:deploy": "prisma migrate deploy",
    "db:dev:rm": "docker compose rm is-it-that-much-dev-db -s -f -v",
    "db:dev:up": "docker compose up is-it-that-much-dev-db -d",
    "db:dev:restart": "npm run db:dev:rm && npm run db:dev:up && sleep 3 && npm run prisma:dev:deploy",
    "prisma:test:deploy": "dotenv -e .env.test -- prisma migrate deploy",
    "db:test:rm": "docker compose rm is-it-that-much-test-db -s -f -v",
    "db:test:up": "docker compose up is-it-that-much-test-db -d",
    "db:test:restart": "npm run db:test:rm && npm run db:test:up && sleep 3 && npm run prisma:test:deploy",
    "prettier": "prettier 'src/**/*.{ts,tsx}' 'prisma/**/*.ts' --write"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
